trap "" 1 2 3
# module setup - should be sourced from every user's .profile
case "$0" in
    -bash|bash|*/bash) . @DEFAULTPATH@/init/bash ;; 
       -ksh|ksh|*/ksh) . @DEFAULTPATH@/init/ksh ;; 
          -sh|sh|*/sh) . @DEFAULTPATH@/init/sh ;; 
                    *) . @DEFAULTPATH@/init/sh ;; 	# default for scripts
esac
ENV=~/.kshenv
export ENV
trap 1 2 3
