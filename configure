    TCLX_LIB_DIR=-L$tclx_libraries
    if test "$UNAME" != ""; then
        os_sys_rel=`$UNAME -rs`
        case "$os_sys_rel" in
        SunOS*5*|HP-UX*) 
            if test "$R_OPTION" = "" ; then
                case "$os_sys_rel" in
                SunOS*) R_OPTION="-R" ;;
                HP-UX*) R_OPTION="-Xlinker +b -Xlinker " ;; 
                esac
             else
                 R_OPTION="${R_OPTION}:"
             fi
             R_OPTION="${R_OPTION}$tclx_libraries" 
             ;;
         Linux*) R_OPTION="${R_OPTION} -Xlinker -rpath -Xlinker $tclx_libraries" ;;
         esac
        echo "$ac_t""R_OPTION set to $R_OPTION for $os_sys_rel" 1>&6
    fi
fi 
