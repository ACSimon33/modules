if ($?tcsh) then
  set modules_shell="tcsh"
else
  set modules_shell="csh"
endif

if ( $?CDS_MIPS_LOCAL ) then
  setenv MODULESHOME $CDS_MIPS_LOCAL/../../envmodule
else
  setenv MODULESHOME /mips/tools/mips/envmodule
endif

if ( $?histchars ) then
  set _histchars = $histchars
  if ($?prompt) then
    alias module 'unset histchars;set _prompt="$prompt";eval `'$MODULESHOME'/modulecmd.tcl '$modules_shell' \!*`;set histchars = $_histchars; set prompt="$_prompt";unset _prompt'
  else
  alias module 'unset histchars;eval `'$MODULESHOME'/modulecmd.tcl '$modules_shell' \!*`;set histchars = $_histchars'
  endif
else
  if ($?prompt) then
    alias module 'set _prompt="$prompt";set prompt="";eval `'$MODULESHOME'/modulecmd.tcl '$modules_shell' \!*`;set prompt="$_prompt";unset _prompt'
else
  alias module 'eval `'$MODULESHOME'/modulecmd.tcl '$modules_shell' \!*`'
  endif
endif


if (! $?MODULEPATH ) then
  setenv MODULEPATH /mips/tools/freeware/modulefiles
endif
