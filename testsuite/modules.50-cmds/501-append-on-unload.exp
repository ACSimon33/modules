$mpre/$mod:

module-whatis\t$mod
append-path\t--append-on-unload FOO /tmp
[escre [msg_moderr {invalid option '--append-on-unload' for append-path} {append-path --append-on-unload FOO /tmp<EXM>} $modfile $ln {  }]]
$modlin"
