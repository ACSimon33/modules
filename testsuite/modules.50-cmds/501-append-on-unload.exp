# refresh
testouterr_cmd sh {refresh} OK {}

unsetenv_loaded_module
unsetenv_var FOO

# whatis
set tserr "$modlin $mpre $modlin
\\s*$mod: $mod"
testouterr_cmd_re sh "whatis $mod" OK $tserr

# display
