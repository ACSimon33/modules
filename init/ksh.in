
MODULESHOME=$MODULESHOME; export MODULESHOME

if [ -z $TCLSH ]; then
   if [ -f /usr/bin/tclsh ]; then
      TCLSH="/usr/bin/tclsh"
   elif [ -f /bin/tclsh ]; then
      TCLSH="/bin/tclsh"
   else 
      TCLSH=""
   fi
fi

moduleraw() { eval `$TCLSH $MODULESHOME/modulecmd.tcl ksh $*`; }
module() { moduleraw $* 2>&1 ; }

. $MODULESHOME/init/modulerc
