if ( $?histchars ) then
   set _histchars = $histchars
   if ($?prompt) then
      alias module 'unset histchars;set _prompt="$prompt";eval `@TCLSH@ @MODULESHOME@/modulecmd.tcl @SHELLNAME@ \!*`;set histchars = $_histchars; set prompt="$_prompt";unset _prompt'
   else
      alias module 'unset histchars;eval `@TCLSH@ @MODULESHOME@/modulecmd.tcl @SHELLNAME@ \!*`;set histchars = $_histchars'
   endif
else
   if ($?prompt) then
      alias module 'set _prompt="$prompt";set prompt="";eval `@TCLSH@ @MODULESHOME@/modulecmd.tcl @SHELLNAME@ \!*`;set prompt="$_prompt";unset _prompt'
   else
      alias module 'eval `@TCLSH@ @MODULESHOME@/modulecmd.tcl @SHELLNAME@ \!*`'
   endif
endif

if ( $?tcsh && $?prompt && -r @MODULESHOME@/init/tcsh_completion ) then
   source @MODULESHOME@/init/tcsh_completion
endif

setenv MODULESHOME @MODULESHOME@

if (! $?LOADEDMODULES ) then
   setenv LOADEDMODULES ''
endif

if ( -r @MODULESHOME@/init/modulerc ) then
   source @MODULESHOME@/init/modulerc
endif
